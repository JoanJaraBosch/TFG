[Unit]
Description=gunicorn daemon
After=network.target

[Service]
Environment=SECRET_KEY=secret
User=joan
Group=www-data
WorkingDirectory=/home/joan/odroid
ExecStart=/home/joan/odroid/myprojectenv/bin/gunicorn --workers 3 --bind unix:/home/joan/odroid/odroid.sock odroid.wsgi:application

[Install]
WantedBy=multi-user.target
