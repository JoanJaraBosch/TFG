[Unit]
Description=gunicorn daemon
After=network.target

[Service]
Environment=SECRET_KEY=secret
User=odroid
Group=www-data
WorkingDirectory=/home/odroid/Downloads/TFG/odroid
ExecStart=/usr/bin/gunicorn --workers 3 --bind unix:/home/odroid/Downloads/TFG/odroid/odroid.sock odroid.wsgi:application

[Install]
WantedBy=multi-user.target
